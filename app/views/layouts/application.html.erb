<!DOCTYPE html>
<html>
<head>
  <title>Blog</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="container">
    <div class="col-sm-3">
      <div id="sider" class="page_wrapper">
        <p class="section">WEBSITE</p>
        <ul class="list-unstyled">
          <li><%= link_to 'All Post', posts_path %></li>
          <li><%= link_to 'About Me', '#' %></li>
        </ul>
        <br>
        <p class="section">SOCITAL</p>
        <ul class="list-unstyled">
          <li><%= link_to 'Twitter', '#' %></li>
          <li><%= link_to 'GitHub', '#' %></li>
          <li><%= link_to 'Email', '#' %></li>
        </ul>
      </div>
    </div>
    <div class="col-sm-9">
      <div class="page_wrapper">
        <%= yield %>
      </div>
    </div>
  </div>
</body>
</html>
